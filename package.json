    "dev:setup": "turbo -F @outly/backend dev:setup"
  },
  "dependencies": {
    "@outly/env": "*",
    "dotenv": "^17.2.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@outly/config": "*",
    "sharp": "^0.34.5",
    "turbo": "^2.6.3",
    "typescript": "^5"
  },
  "packageManager": "npm@11.7.0",
  "overrides": {
    "react": "19.1.0",
    "react-dom": "19.1.0"
  }
}
